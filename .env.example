NEXT_PUBLIC_ENABLE_GITHUB_OAUTH=false
NEXT_PUBLIC_ENABLE_GOOGLE_CALENDAR_OAUTH=false
NEXT_PUBLIC_ENABLE_RSS_INTEGRATION=false
